    "@adriand94/jobber-shared": "^0.0.1",
    "@elastic/elasticsearch": "^8.11.0",
    "amqplib": "^0.10.3",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "email-templates": "^11.1.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.3.0",
    "nodemailer": "^6.9.7",
    "npm": "^10.2.5",
    "pino-pretty": "^10.3.1",
    "minimatch": "3.0.8",
    "shelljs": "^0.8.5",
    "typescript": "^5.3.3",
    "typescript-transform-paths": "^3.4.6",
    "winston": "^3.11.0"
  }
